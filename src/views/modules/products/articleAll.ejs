

<% listAll.forEach(product => { %>
   
    <article class ="main_article">
        
<!--         el default.jpg puede ser cualquire imagen q sera la generica cuando no se cargue imagen
 -->        
        
        <img src="<%= product.image ? product.image : "default.jgp" %> " alt="cafetera" id="bordeinferior">
        <picture class="camion" ><i  class="fas fa-truck" ></i></picture>     
        <p class ="precios">$ <%=product.price %> </p>
        <p class ="descuentos"> <%=product.discount %> off</p>
        <p class = "descripciones"><%= product.description %></p>   

    </article>
<% }) %>